<h1>Current list of specialities:</h1>

@if (@Model.Count == 0)
{
    <h3>No specialties have been added to the list yet!</h3>
}
@if (Model.Count != 0)
{
    <ul>
    @foreach (var specialty in Model)
    {
        <h3><a href='/specialty/@specialty.GetId()'>@specialty.GetSpecialty()</a></h3>
    }
    </ul>
}

<h3><a href='/specialty/new'>Add a new specialty</a></h3>
<h3><p><a href='/stylists'>Go to the list of stylists to add a new specialty to a stylist</a></p></h3>
<p><a href='/'>Bakc to Home</a></p>
